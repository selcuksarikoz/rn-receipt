{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src/",
    "importHelpers": true,
    "paths": {
      "@components": [ "components/" ],
      "@components/*": [ "components/*" ],
      "@api": [ "api" ],
      "@api/*": [ "api/*" ],
      "@screens": [ "screens" ],
      "@screens/*": [ "screens/*" ],
      "@constants": [ "constants" ],
      "@constants/*": [ "constants/*" ],
      "@utils": [ "utils" ],
      "@utils/*": [ "utils/*" ]
    }
  }
}
